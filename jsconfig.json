{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
     "baseUrl": "src",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/app/(pages)/layout.jsx",
    "src/app/(pages)/dashboard/administrative/dataDirectory/page.js",
    "src/app/components/dashboard/administrative/data directory/newMatch.jsx",
    "src/app/page.jsx"
  ],
  "exclude": ["node_modules"]
}
